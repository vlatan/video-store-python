<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="{{ url_for('main.sitemap_style') }}"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for loc, lastmod in data.items() %}
    <sitemap>
        <loc>{{ loc }}</loc>
        <lastmod>{{ lastmod }}</lastmod>
    </sitemap>
    {% endfor %}
</sitemapindex>